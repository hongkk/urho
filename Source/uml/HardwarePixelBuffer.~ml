<XPD:ATTR name="Text" type="string">void GLHardwarePixelBuffer::blitFromMemory(const PixelBox &amp;src, const Box &amp;dstBox)
{
 .....

  upload(scaled, dstBox);
  
  freeBuffer();
}

void GLHardwarePixelBuffer::blitToMemory(const Box &amp;srcBox, const PixelBox &amp;dst)
{
  ......
  download(dst);
}


void GLTextureBuffer::upload(const PixelBox &amp;data, const Box &amp;dest)
{
  glCompressedTexImage2DARB() //将压缩纹理数据从内存上传到显存
  glTexSubImage2D() //将纹理数据从内存上传到显存
}

void GLTextureBuffer::download(const PixelBox &amp;data)
{
  glGetCompressedTexImageARB()  //从显存中得到压缩纹理数据
  glGetTexImage()  //从显存中得到纹理数据
}
